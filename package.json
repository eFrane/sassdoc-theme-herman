        "prettier --write",
        "git add"
      ],
      "{gulpfile,index,.babelrc,postcss.config,sass-json-loader,webpack.config}.js": [
        "prettier --write",
        "git add"
      ],
      "*.scss": [
        "prettier --write",
        "sass-lint -v -q",
